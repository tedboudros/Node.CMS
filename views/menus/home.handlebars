                        <div class="col">
                            <div>
                                <span class="uk-margin-small-right uk-text-emphasis" uk-icon="grid"></span>
                                <a>Status</a>
                            </div>
                            <br>
                            <div>
                                <span class="uk-margin-small-right uk-text-emphasis" uk-icon="icon: cog"></span>
